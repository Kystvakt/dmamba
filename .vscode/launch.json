    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Train with Hydra",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/train.py",  // 스크립트 경로
        "console": "integratedTerminal",
        "args": [
